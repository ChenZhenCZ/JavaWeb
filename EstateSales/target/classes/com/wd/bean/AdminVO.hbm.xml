<?xml version="1.0"?>    
<!DOCTYPE hibernate-mapping PUBLIC     
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"    
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">       
<hibernate-mapping>      
    <class name="com.wd.bean.AdminVO" table="admin">
			<id name="id" column="id" type="int">
				<generator class="native"></generator>
			</id>
			<property name="adminName"  type="string"></property>
			<property name="adminPwd"  type="string"></property>
		</class>
</hibernate-mapping>